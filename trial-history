vim gogs-postgresql.yaml
oc projects
oc create -f gogs-postgresql.yaml -n openshift
oc get pods -n openshift
oc get pods -n pro1
oc create gogs-postgresql.yaml -n pro1
oc create -f gogs-postgresql.yaml -n pro1
oc get route
oc get route -n pro1
oc get route -n openshift
oc delete all -l app=gogs-postgresql.yaml -n pro1
oc delete all -l app=gogs-postgresql.yaml
oc project pro1
oc get route
oc get pods
oc get svc
oc describe dc gogs
oc describe dc gogs | less
oc describe dc postgresql
oc get route
touch README.md
git init
